{
    "name": "sayan/wp-auto-republish",
    "description": "The WP Auto Republish plugin helps revive old posts by resetting the publish date to the current date. This will push old posts to your front page, the top of archive pages, and back into RSS feeds. Ideal for sites with a large repository of evergreen content.",
    "type": "wordpress-plugin",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Sayan Datta",
            "email": "hello@sayandatta.in",
            "homepage": "http://www.sayandatta.in",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "support@wpautorepublish.com"
    },
    "funding": [
        {
            "type": "Paypal",
            "url": "https://paypal.me/iamsayan"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Wpar\\": "./includes"
        },
        "files": ["vendor/woocommerce/action-scheduler/action-scheduler.php"]
    },
    "require": {
        "abraham/twitteroauth": "2.0.0",
        "facebook/graph-sdk": "^5.7",
        "tumblr/tumblr": "0.4.0",
        "zoonman/linkedin-api-php-client": "^0.0.14",
        "woocommerce/action-scheduler": "^3.1"
    }
}
